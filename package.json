{
  "name": "swagger-transform",
  "version": "1.0.0",
  "description": "A tool to generate API clients from Swagger/OpenAPI specs.",
  "author": "tolking <qw13131wang@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "dev": "rollup -c rollup.config.js -w",
    "build": "rollup -c rollup.config.js",
    "test": "node test/index.js",
    "lint": "eslint --fix",
    "prepare": "husky"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolking/swagger-to-api.git"
  },
  "bugs": {
    "url": "https://github.com/tolking/swagger-to-api/issues"
  },
  "homepage": "https://github.com/tolking/swagger-to-api#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.7",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "rollup": "^4.17.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "lint-staged": {
    "*.{ts,vue,js,tsx,jsx}": [
      "eslint --fix"
    ]
  }
}
